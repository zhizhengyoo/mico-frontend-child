{
  "name": "micro-app2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-my-comp": "ng build --prod --outputHashing=none --single-bundle --keep-polyfills",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "concat-es5": "concat -o output/output-es5.js ./dist/app2/runtime-es5.js ./dist/app2/polyfills-es5.js ./dist/app2/main-es5.js",
    "concat-es2015": "concat -o output/output-es2015.js ./dist/app2/runtime-es2015.js ./dist/app2/polyfills-es2015.js ./dist/app2/main-es2015.js",
    "concat": "npm run concat-es5 && npm run concat-es2015",
    "_build": "ng build --prod --aot --output-hashing=none && npm run concat"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "^10.2.2",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/elements": "^9.1.12",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@worktile/planet": "^1.2.0",
    "concat": "^1.0.3",
    "ng-zorro-antd": "^9.3.0",
    "ngx-build-plus": "^10.1.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
